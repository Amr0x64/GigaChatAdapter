<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyVersion>0.1.0</AssemblyVersion>
    <FileVersion>0.1.0</FileVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>reNNN</Authors>
    <RepositoryUrl>https://github.com/reNNN/GigaChatAdapter</RepositoryUrl>
    <Description>GigaChat API easily. Unofficial .net library to use GigaChat by Sber in your apps. Example in GitHub</Description>
    <PackageTags>GigaChat</PackageTags>
    <PackageProjectUrl>https://github.com/reNNN/GigaChatAdapter</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Encoding.CodePages" Version="6.0.0" />
  </ItemGroup>

</Project>
